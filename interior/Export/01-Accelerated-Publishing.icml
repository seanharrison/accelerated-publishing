			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]" PageNumberType="TextVariable" AppliedConditions="Condition/Print">
				<CrossReferenceSource Self="u13b2" AppliedFormat="u9f" Name="Hyperlink 108" Hidden="false" AppliedCharacterStyle="n">
